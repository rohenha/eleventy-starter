{
  "name": "eleventy-plus",
  "version": "1.0.0",
  "description": "Eleventy starter with twig and viteJS",
  "main": "index.js",
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ],
  "scripts": {
    "watch": "eleventy --watch",
    "dev": "eleventy --serve --incremental",
    "bench": "DEBUG=Eleventy:Benchmark* eleventy",
    "build": " eleventy",
    "clean": "del-cli _dist",
    "sprite": "node utils/sprites",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eleventy",
    "twig",
    "vite",
    "vanillajs",
    "jamstack"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0"
  },
  "author": "rohenha",
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0-beta.1",
    "@11ty/eleventy-img": "^5.0.0-beta.9",
    "@11ty/eleventy-plugin-vite": "^5.0.0-alpha.1",
    "@barba/core": "^2.10.0",
    "@factorial/eleventy-plugin-twig": "^0.1.3",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "animejs": "^3.2.2",
    "autoprefixer": "^10.4.19",
    "del-cli": "^5.1.0",
    "dotenv": "^16.4.5",
    "locomotive-scroll": "5.0.0-beta.13",
    "modujs": "^1.4.2",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-inline-svg": "^6.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "rollup-plugin-critical": "^1.0.13",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "sharp": "^0.33.4",
    "stats.js": "^0.17.0",
    "svg-sprite": "^2.0.4",
    "twig": "^1.17.1",
    "vanilla-lazyload": "^19.1.3",
    "vite": "^5.3.5",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-sass-glob-import": "^3.0.2"
  }
}
